{
  "name": "caibook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --ci --runInBand",
    "tests": "jest --watch",
    "tests:watch": "jest --watch",
    "cy:run": "cross-env APP_BASE_URL=http://localhost:3000 NEXT_PUBLIC_API_MODE=mock start-server-and-test \"npm run dev\" http://localhost:3000 \"npx cypress run\"",
    "cy:open": "cross-env APP_BASE_URL=http://localhost:3000 NEXT_PUBLIC_API_MODE=mock start-server-and-test \"npm run dev\" http://localhost:3000 \"npx cypress open\""
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.10.0",
    "@hookform/resolvers": "^5.2.1",
    "@vercel/analytics": "^1.5.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "cross-env": "^10.1.0",
    "cypress": "^15.6.0",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "^4.1.13",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
